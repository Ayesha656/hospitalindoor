<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".appointmenta">
    <androidx.cardview.widget.CardView
        android:id="@+id/cardView4"
        android:layout_width="411dp"
        android:layout_height="101dp"
        app:cardBackgroundColor="@color/blue"
        app:cardCornerRadius="26dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView26"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/opensansextrabold"
                android:text="Doctor Appointment"
                android:textColor="@color/white"
                android:textSize="24dp"

                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/opensanssemibold"
        android:text="Find your desired specialist"
        android:textColor="@color/black"
        android:textSize="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.166"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.204" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <HorizontalScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:ignore="SpeakableTextPresentCheck">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="horizontal"

                android:padding="15dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/heart1"
                    android:layout_width="match_parent"
                    android:layout_height="128dp"
                    android:layout_margin="12dp"
                    app:cardCornerRadius="15dp">

                    <ImageButton
                        android:id="@+id/imageButton6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cornerradius"
                        android:backgroundTint="@color/pink"
                        android:src="@drawable/heartrateico"
                        tools:ignore="SpeakableTextPresentCheck" />

                    <TextView
                        android:id="@+id/textView29"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:layout_margin="12dp"
                        android:fontFamily="@font/opensanssemibold"
                        android:text="Heart"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/brain"
                    android:layout_width="match_parent"
                    android:layout_height="128dp"
                    android:layout_margin="12dp"
                    app:cardCornerRadius="15dp">

                    <ImageButton
                        android:id="@+id/imageButton7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cornerradius"
                        android:backgroundTint="@color/orange"
                        android:src="@drawable/brainico"
                        tools:ignore="SpeakableTextPresentCheck" />

                    <TextView
                        android:id="@+id/textView30"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:layout_margin="12dp"
                        android:fontFamily="@font/opensanssemibold"
                        android:text="Brain"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/dental"
                    android:layout_width="match_parent"
                    android:layout_height="128dp"
                    android:layout_margin="12dp"
                    app:cardCornerRadius="15dp">

                    <ImageButton
                        android:id="@+id/imageButton8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cornerradius"
                        android:backgroundTint="@color/lightblue"
                        android:src="@drawable/toothico"
                        tools:ignore="SpeakableTextPresentCheck" />

                    <TextView
                        android:id="@+id/textView31"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:layout_margin="12dp"
                        android:fontFamily="@font/opensanssemibold"
                        android:text="Dental"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/lung"
                    android:layout_width="match_parent"
                    android:layout_height="128dp"
                    android:layout_margin="12dp"
                    app:cardCornerRadius="15dp">

                    <ImageButton
                        android:id="@+id/imageButton9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cornerradius"
                        android:backgroundTint="@color/lightred"
                        android:src="@drawable/lungsico"
                        tools:ignore="SpeakableTextPresentCheck" />

                    <TextView
                        android:id="@+id/textView32"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:layout_margin="12dp"
                        android:fontFamily="@font/opensanssemibold"
                        android:text="Lungs"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/neuro"
                    android:layout_width="match_parent"
                    android:layout_height="128dp"
                    android:layout_margin="12dp"
                    app:cardCornerRadius="15dp">

                    <ImageButton
                        android:id="@+id/imageButton10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cornerradius"
                        android:backgroundTint="@color/lightgreen"
                        android:src="@drawable/discoverico"
                        tools:ignore="SpeakableTextPresentCheck" />

                    <TextView
                        android:id="@+id/textView33"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:layout_margin="12dp"
                        android:fontFamily="@font/opensanssemibold"
                        android:text="Search All"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>
            </LinearLayout>
        </HorizontalScrollView>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView5">

        <HorizontalScrollView
            android:layout_width="match_parent"
            android:layout_height="279dp"
            tools:ignore="SpeakableTextPresentCheck">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/blue"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:id="@+id/doctor1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="12dp"
                    android:background="@drawable/cornerradius"
                    android:backgroundTint="@color/lightblue"
                    app:cardCornerRadius="12dp">

                    <TextView
                        android:id="@+id/textView40"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/opensanssemibold"
                        android:gravity="center_horizontal"
                        android:text="DR. JAMES"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="14dp" />

                    <ImageView
                        android:id="@+id/imageView5"
                        android:layout_width="200dp"
                        android:layout_height="200dp"
                        android:src="@drawable/doctora" />

                    <TextView
                        android:id="@+id/textView37."
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:text="HEART SURGEON"
                        android:textColor="@color/blue"
                        android:textSize="14dp" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/doctor2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="12dp"
                    android:background="@drawable/cornerradius"
                    android:backgroundTint="@color/pink"
                    app:cardCornerRadius="12dp">

                    <TextView
                        android:id="@+id/textView36"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/opensanssemibold"
                        android:gravity="center_horizontal"
                        android:text="DR. MARIS"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="14dp" />

                    <ImageView
                        android:id="@+id/imageView7"
                        android:layout_width="200dp"
                        android:layout_height="200dp"
                        android:src="@drawable/doctorc" />

                    <TextView
                        android:id="@+id/textView37"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:text="DENTAL"
                        android:textColor="@color/blue"
                        android:textSize="14dp" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/doctor3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="12dp"
                    android:background="@drawable/cornerradius"
                    android:backgroundTint="@color/lightblue"
                    app:cardCornerRadius="12dp">

                    <TextView
                        android:id="@+id/textView39"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/opensanssemibold"
                        android:gravity="center_horizontal"
                        android:text="DR. MOIZ"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="14dp" />

                    <ImageView
                        android:id="@+id/imageView8"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/doctorb" />

                    <TextView
                        android:id="@+id/textView38"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:text="KIDNEY"
                        android:textColor="@color/blue"
                        android:textSize="14dp" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/doctor4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="12dp"
                    android:background="@drawable/cornerradius"
                    android:backgroundTint="@color/lightblue"
                    app:cardCornerRadius="12dp">

                    <TextView
                        android:id="@+id/textView41"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/opensanssemibold"
                        android:gravity="center_horizontal"
                        android:text="DR. ALI"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="14dp" />

                    <ImageView
                        android:id="@+id/imageView9"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/doctora" />

                    <TextView
                        android:id="@+id/textView42"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:text="NEUROLOGIST"
                        android:textColor="@color/blue"
                        android:textSize="14dp" />
                </androidx.cardview.widget.CardView>


            </LinearLayout>


        </HorizontalScrollView>


    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>